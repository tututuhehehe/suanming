{
  "yao_ci": [
    {"hexagram_id": 1, "position": 1, "name": "初九", "text": "潜龙勿用。", "xiang_zhuan": "潜龙勿用，阳在下也。"},
    {"hexagram_id": 1, "position": 2, "name": "九二", "text": "见龙在田，利见大人。", "xiang_zhuan": "见龙在田，德施普也。"},
    {"hexagram_id": 1, "position": 3, "name": "九三", "text": "君子终日乾乾，夕惕若厉，无咎。", "xiang_zhuan": "终日乾乾，反复道也。"},
    {"hexagram_id": 1, "position": 4, "name": "九四", "text": "或跃在渊，无咎。", "xiang_zhuan": "或跃在渊，进无咎也。"},
    {"hexagram_id": 1, "position": 5, "name": "九五", "text": "飞龙在天，利见大人。", "xiang_zhuan": "飞龙在天，大人造也。"},
    {"hexagram_id": 1, "position": 6, "name": "上九", "text": "亢龙有悔。", "xiang_zhuan": "亢龙有悔，盈不可久也。"},
    {"hexagram_id": 1, "position": 0, "name": "用九", "text": "见群龙无首，吉。", "xiang_zhuan": "用九，天德不可为首也。"},
    
    {"hexagram_id": 2, "position": 1, "name": "初六", "text": "履霜，坚冰至。", "xiang_zhuan": "履霜坚冰，阴始凝也。驯致其道，至坚冰也。"},
    {"hexagram_id": 2, "position": 2, "name": "六二", "text": "直、方、大，不习无不利。", "xiang_zhuan": "六二之动，直以方也。不习无不利，地道光也。"},
    {"hexagram_id": 2, "position": 3, "name": "六三", "text": "含章可贞。或从王事，无成有终。", "xiang_zhuan": "含章可贞，以时发也。或从王事，知光大也。"},
    {"hexagram_id": 2, "position": 4, "name": "六四", "text": "括囊，无咎，无誉。", "xiang_zhuan": "括囊无咎，慎不害也。"},
    {"hexagram_id": 2, "position": 5, "name": "六五", "text": "黄裳，元吉。", "xiang_zhuan": "黄裳元吉，文在中也。"},
    {"hexagram_id": 2, "position": 6, "name": "上六", "text": "龙战于野，其血玄黄。", "xiang_zhuan": "龙战于野，其道穷也。"},
    {"hexagram_id": 2, "position": 0, "name": "用六", "text": "利永贞。", "xiang_zhuan": "用六永贞，以大终也。"},
    
    {"hexagram_id": 3, "position": 1, "name": "初九", "text": "磐桓，利居贞，利建侯。", "xiang_zhuan": "虽磐桓，志行正也。以贵下贱，大得民也。"},
    {"hexagram_id": 3, "position": 2, "name": "六二", "text": "屯如邅如，乘马班如。匪寇婚媾，女子贞不字，十年乃字。", "xiang_zhuan": "六二之难，乘刚也。十年乃字，反常也。"},
    {"hexagram_id": 3, "position": 3, "name": "六三", "text": "即鹿无虞，惟入于林中，君子几不如舍，往吝。", "xiang_zhuan": "即鹿无虞，以从禽也。君子舍之，往吝穷也。"},
    {"hexagram_id": 3, "position": 4, "name": "六四", "text": "乘马班如，求婚媾，往吉无不利。", "xiang_zhuan": "求而往，明也。"},
    {"hexagram_id": 3, "position": 5, "name": "九五", "text": "屯其膏，小贞吉，大贞凶。", "xiang_zhuan": "屯其膏，施未光也。"},
    {"hexagram_id": 3, "position": 6, "name": "上六", "text": "乘马班如，泣血涟如。", "xiang_zhuan": "泣血涟如，何可长也。"},
    
    {"hexagram_id": 4, "position": 1, "name": "初六", "text": "发蒙，利用刑人，用说桎梏，以往吝。", "xiang_zhuan": "利用刑人，以正法也。"},
    {"hexagram_id": 4, "position": 2, "name": "九二", "text": "包蒙吉；纳妇吉；子克家。", "xiang_zhuan": "子克家，刚柔接也。"},
    {"hexagram_id": 4, "position": 3, "name": "六三", "text": "勿用取女；见金夫，不有躬，无攸利。", "xiang_zhuan": "勿用取女，行不顺也。"},
    {"hexagram_id": 4, "position": 4, "name": "六四", "text": "困蒙，吝。", "xiang_zhuan": "困蒙之吝，独远实也。"},
    {"hexagram_id": 4, "position": 5, "name": "六五", "text": "童蒙，吉。", "xiang_zhuan": "童蒙之吉，顺以巽也。"},
    {"hexagram_id": 4, "position": 6, "name": "上九", "text": "击蒙；不利为寇，利御寇。", "xiang_zhuan": "利用御寇，上下顺也。"},
    
    {"hexagram_id": 5, "position": 1, "name": "初九", "text": "需于郊。利用恒，无咎。", "xiang_zhuan": "需于郊，不犯难行也。利用恒，无咎；未失常也。"},
    {"hexagram_id": 5, "position": 2, "name": "九二", "text": "需于沙。小有言，终吉。", "xiang_zhuan": "需于沙，衍在中也。虽小有言，以终吉也。"},
    {"hexagram_id": 5, "position": 3, "name": "九三", "text": "需于泥，致寇至。", "xiang_zhuan": "需于泥，灾在外也。自我致寇，敬慎不败也。"},
    {"hexagram_id": 5, "position": 4, "name": "六四", "text": "需于血，出自穴。", "xiang_zhuan": "需于血，顺以听也。"},
    {"hexagram_id": 5, "position": 5, "name": "九五", "text": "需于酒食，贞吉。", "xiang_zhuan": "酒食贞吉，以中正也。"},
    {"hexagram_id": 5, "position": 6, "name": "上六", "text": "入于穴，有不速之客三人来，敬之终吉。", "xiang_zhuan": "不速之客来，敬之终吉。虽不当位，未大失也。"},
    
    {"hexagram_id": 6, "position": 1, "name": "初六", "text": "不永所事，小有言，终吉。", "xiang_zhuan": "不永所事，讼不可长也。虽有小言，其辩明也。"},
    {"hexagram_id": 6, "position": 2, "name": "九二", "text": "不克讼，归而逋，其邑人三百户，无眚。", "xiang_zhuan": "不克讼，归逋窜也。自下讼上，患至掇也。"},
    {"hexagram_id": 6, "position": 3, "name": "六三", "text": "食旧德，贞厉，终吉，或从王事，无成。", "xiang_zhuan": "食旧德，从上吉也。"},
    {"hexagram_id": 6, "position": 4, "name": "九四", "text": "不克讼，复既命，渝安贞，吉。", "xiang_zhuan": "复即命，渝安贞；不失也。"},
    {"hexagram_id": 6, "position": 5, "name": "九五", "text": "讼元吉。", "xiang_zhuan": "讼元吉，以中正也。"},
    {"hexagram_id": 6, "position": 6, "name": "上九", "text": "或锡之鞶带，终朝三褫之。", "xiang_zhuan": "以讼受服，亦不足敬也。"},
    
    {"hexagram_id": 7, "position": 1, "name": "初六", "text": "师出以律，否臧凶。", "xiang_zhuan": "师出以律，失律凶也。"},
    {"hexagram_id": 7, "position": 2, "name": "九二", "text": "在师中，吉无咎，王三锡命。", "xiang_zhuan": "在师中吉，承天宠也。王三锡命，怀万邦也。"},
    {"hexagram_id": 7, "position": 3, "name": "六三", "text": "师或舆尸，凶。", "xiang_zhuan": "师或舆尸，大无功也。"},
    {"hexagram_id": 7, "position": 4, "name": "六四", "text": "师左次，无咎。", "xiang_zhuan": "左次无咎，未失常也。"},
    {"hexagram_id": 7, "position": 5, "name": "六五", "text": "田有禽，利执言，无咎。长子帅师，弟子舆尸，贞凶。", "xiang_zhuan": "长子帅师，以中行也。弟子舆尸，使不当也。"},
    {"hexagram_id": 7, "position": 6, "name": "上六", "text": "大君有命，开国承家，小人勿用。", "xiang_zhuan": "大君有命，以正功也。小人勿用，必乱邦也。"},
    
    {"hexagram_id": 8, "position": 1, "name": "初六", "text": "有孚比之，无咎。有孚盈缶，终来有它，吉。", "xiang_zhuan": "比之初六，有它吉也。"},
    {"hexagram_id": 8, "position": 2, "name": "六二", "text": "比之自内，贞吉。", "xiang_zhuan": "比之自内，不自失也。"},
    {"hexagram_id": 8, "position": 3, "name": "六三", "text": "比之匪人。", "xiang_zhuan": "比之匪人，不亦伤乎！"},
    {"hexagram_id": 8, "position": 4, "name": "六四", "text": "外比之，贞吉。", "xiang_zhuan": "外比于贤，以从上也。"},
    {"hexagram_id": 8, "position": 5, "name": "九五", "text": "显比，王用三驱，失前禽。邑人不诫，吉。", "xiang_zhuan": "显比之吉，位正中也。舍逆取顺，失前禽也。邑人不诫，上使中也。"},
    {"hexagram_id": 8, "position": 6, "name": "上六", "text": "比之无首，凶。", "xiang_zhuan": "比之无首，无所终也。"},
    
    {"hexagram_id": 9, "position": 1, "name": "初九", "text": "复自道，何其咎，吉。", "xiang_zhuan": "复自道，其义吉也。"},
    {"hexagram_id": 9, "position": 2, "name": "九二", "text": "牵复，吉。", "xiang_zhuan": "牵复在中，亦不自失也。"},
    {"hexagram_id": 9, "position": 3, "name": "九三", "text": "舆说辐，夫妻反目。", "xiang_zhuan": "夫妻反目，不能正室也。"},
    {"hexagram_id": 9, "position": 4, "name": "六四", "text": "有孚，血去惕出，无咎。", "xiang_zhuan": "有孚惕出，上合志也。"},
    {"hexagram_id": 9, "position": 5, "name": "九五", "text": "有孚挛如，富以其邻。", "xiang_zhuan": "有孚挛如，不独富也。"},
    {"hexagram_id": 9, "position": 6, "name": "上九", "text": "既雨既处，尚德载，妇贞厉。月几望，君子征凶。", "xiang_zhuan": "既雨既处，德积载也。君子征凶，有所疑也。"},
    
    {"hexagram_id": 10, "position": 1, "name": "初九", "text": "素履往，无咎。", "xiang_zhuan": "素履之往，独行愿也。"},
    {"hexagram_id": 10, "position": 2, "name": "九二", "text": "履道坦坦，幽人贞吉。", "xiang_zhuan": "幽人贞吉，中不自乱也。"},
    {"hexagram_id": 10, "position": 3, "name": "六三", "text": "眇能视，跛能履，履虎尾，咥人，凶。武人为于大君。", "xiang_zhuan": "眇能视，不足以有明也。跛能履，不足以与行也。咥人之凶，位不当也。武人为于大君，志刚也。"},
    {"hexagram_id": 10, "position": 4, "name": "九四", "text": "履虎尾，愬愬终吉。", "xiang_zhuan": "愬愬终吉，志行也。"},
    {"hexagram_id": 10, "position": 5, "name": "九五", "text": "夬履，贞厉。", "xiang_zhuan": "夬履贞厉，位正当也。"},
    {"hexagram_id": 10, "position": 6, "name": "上九", "text": "视履考祥，其旋元吉。", "xiang_zhuan": "元吉在上，大有庆也。"},
    
    {"hexagram_id": 11, "position": 1, "name": "初九", "text": "拔茅茹，以其汇，征吉。", "xiang_zhuan": "拔茅征吉，志在外也。"},
    {"hexagram_id": 11, "position": 2, "name": "九二", "text": "包荒，用冯河，不遐遗，朋亡，得尚于中行。", "xiang_zhuan": "包荒，得尚于中行，以光大也。"},
    {"hexagram_id": 11, "position": 3, "name": "九三", "text": "无平不陂，无往不复，艰贞无咎。勿恤其孚，于食有福。", "xiang_zhuan": "无往不复，天地际也。"},
    {"hexagram_id": 11, "position": 4, "name": "六四", "text": "翩翩不富，以其邻，不戒以孚。", "xiang_zhuan": "翩翩不富，皆失实也。不戒以孚，中心愿也。"},
    {"hexagram_id": 11, "position": 5, "name": "六五", "text": "帝乙归妹，以祉元吉。", "xiang_zhuan": "以祉元吉，中以行愿也。"},
    {"hexagram_id": 11, "position": 6, "name": "上六", "text": "城复于隍，勿用师。自邑告命，贞吝。", "xiang_zhuan": "城复于隍，其命乱也。"},
    
    {"hexagram_id": 12, "position": 1, "name": "初六", "text": "拔茅茹，以其汇，贞吉亨。", "xiang_zhuan": "拔茅贞吉，志在君也。"},
    {"hexagram_id": 12, "position": 2, "name": "六二", "text": "包承。小人吉，大人否亨。", "xiang_zhuan": "大人否亨，不乱群也。"},
    {"hexagram_id": 12, "position": 3, "name": "六三", "text": "包羞。", "xiang_zhuan": "包羞，位不当也。"},
    {"hexagram_id": 12, "position": 4, "name": "九四", "text": "有命无咎，畴离祉。", "xiang_zhuan": "有命无咎，志行也。"},
    {"hexagram_id": 12, "position": 5, "name": "九五", "text": "休否，大人吉。其亡其亡，系于苞桑。", "xiang_zhuan": "大人之吉，位正当也。"},
    {"hexagram_id": 12, "position": 6, "name": "上九", "text": "倾否，先否后喜。", "xiang_zhuan": "否终则倾，何可长也。"},
    
    {"hexagram_id": 13, "position": 1, "name": "初九", "text": "同人于门，无咎。", "xiang_zhuan": "出门同人，又谁咎也。"},
    {"hexagram_id": 13, "position": 2, "name": "六二", "text": "同人于宗，吝。", "xiang_zhuan": "同人于宗，吝道也。"},
    {"hexagram_id": 13, "position": 3, "name": "九三", "text": "伏戎于莽，升其高陵，三岁不兴。", "xiang_zhuan": "伏戎于莽，敌刚也。三岁不兴，安行也。"},
    {"hexagram_id": 13, "position": 4, "name": "九四", "text": "乘其墉，弗克攻，吉。", "xiang_zhuan": "乘其墉，义弗克也，其吉，则困而反则也。"},
    {"hexagram_id": 13, "position": 5, "name": "九五", "text": "同人，先号啕而后笑，大师克相遇。", "xiang_zhuan": "同人之先，以中直也。大师相遇，言相克也。"},
    {"hexagram_id": 13, "position": 6, "name": "上九", "text": "同人于郊，无悔。", "xiang_zhuan": "同人于郊，志未得也。"},
    
    {"hexagram_id": 14, "position": 1, "name": "初九", "text": "无交害，匪咎，艰则无咎。", "xiang_zhuan": "大有初九，无交害也。"},
    {"hexagram_id": 14, "position": 2, "name": "九二", "text": "大车以载，有攸往，无咎。", "xiang_zhuan": "大车以载，积中不败也。"},
    {"hexagram_id": 14, "position": 3, "name": "九三", "text": "公用亨于天子，小人弗克。", "xiang_zhuan": "公用亨于天子，小人害也。"},
    {"hexagram_id": 14, "position": 4, "name": "九四", "text": "匪其彭，无咎。", "xiang_zhuan": "匪其彭，无咎；明辨晰也。"},
    {"hexagram_id": 14, "position": 5, "name": "六五", "text": "厥孚交如，威如；吉。", "xiang_zhuan": "厥孚交如，信以发志也。威如之吉，易而无备也。"},
    {"hexagram_id": 14, "position": 6, "name": "上九", "text": "自天祐之，吉无不利。", "xiang_zhuan": "大有上吉，自天祐也。"},
    
    {"hexagram_id": 15, "position": 1, "name": "初六", "text": "谦谦君子，用涉大川，吉。", "xiang_zhuan": "谦谦君子，卑以自牧也。"},
    {"hexagram_id": 15, "position": 2, "name": "六二", "text": "鸣谦，贞吉。", "xiang_zhuan": "鸣谦贞吉，中心得也。"},
    {"hexagram_id": 15, "position": 3, "name": "九三", "text": "劳谦君子，有终吉。", "xiang_zhuan": "劳谦君子，万民服也。"},
    {"hexagram_id": 15, "position": 4, "name": "六四", "text": "无不利，撝谦。", "xiang_zhuan": "无不利，撝谦；不违则也。"},
    {"hexagram_id": 15, "position": 5, "name": "六五", "text": "不富，以其邻，利用侵伐，无不利。", "xiang_zhuan": "利用侵伐，征不服也。"},
    {"hexagram_id": 15, "position": 6, "name": "上六", "text": "鸣谦，利用行师，征邑国。", "xiang_zhuan": "鸣谦，志未得也。可用行师，征邑国也。"},
    
    {"hexagram_id": 16, "position": 1, "name": "初六", "text": "鸣豫，凶。", "xiang_zhuan": "初六鸣豫，志穷凶也。"},
    {"hexagram_id": 16, "position": 2, "name": "六二", "text": "介于石，不终日，贞吉。", "xiang_zhuan": "不终日，贞吉；以中正也。"},
    {"hexagram_id": 16, "position": 3, "name": "六三", "text": "盱豫，悔。迟有悔。", "xiang_zhuan": "盱豫有悔，位不当也。"},
    {"hexagram_id": 16, "position": 4, "name": "九四", "text": "由豫，大有得。勿疑。朋盍簪。", "xiang_zhuan": "由豫，大有得；志大行也。"},
    {"hexagram_id": 16, "position": 5, "name": "六五", "text": "贞疾，恒不死。", "xiang_zhuan": "六五贞疾，乘刚也。恒不死，中未亡也。"},
    {"hexagram_id": 16, "position": 6, "name": "上六", "text": "冥豫，成有渝，无咎。", "xiang_zhuan": "冥豫在上，何可长也。"},
    
    {"hexagram_id": 17, "position": 1, "name": "初九", "text": "官有渝，贞吉。出门交有功。", "xiang_zhuan": "官有渝，从正吉也。出门交有功，不失也。"},
    {"hexagram_id": 17, "position": 2, "name": "六二", "text": "系小子，失丈夫。", "xiang_zhuan": "系小子，弗兼与也。"},
    {"hexagram_id": 17, "position": 3, "name": "六三", "text": "系丈夫，失小子。随有求得，利居贞。", "xiang_zhuan": "系丈夫，志舍下也。"},
    {"hexagram_id": 17, "position": 4, "name": "九四", "text": "随有获，贞凶。有孚在道，以明，何咎。", "xiang_zhuan": "随有获，其义凶也。有孚在道，明功也。"},
    {"hexagram_id": 17, "position": 5, "name": "九五", "text": "孚于嘉，吉。", "xiang_zhuan": "孚于嘉，吉；位正中也。"},
    {"hexagram_id": 17, "position": 6, "name": "上六", "text": "拘系之，乃从维之。王用亨于西山。", "xiang_zhuan": "拘系之，上穷也。"},
    
    {"hexagram_id": 18, "position": 1, "name": "初六", "text": "干父之蛊，有子，考无咎，厉终吉。", "xiang_zhuan": "干父之蛊，意承考也。"},
    {"hexagram_id": 18, "position": 2, "name": "九二", "text": "干母之蛊，不可贞。", "xiang_zhuan": "干母之蛊，得中道也。"},
    {"hexagram_id": 18, "position": 3, "name": "九三", "text": "干父小有悔，无大咎。", "xiang_zhuan": "干父之蛊，终无咎也。"},
    {"hexagram_id": 18, "position": 4, "name": "六四", "text": "裕父之蛊，往见吝。", "xiang_zhuan": "裕父之蛊，往未得也。"},
    {"hexagram_id": 18, "position": 5, "name": "六五", "text": "干父之蛊，用誉。", "xiang_zhuan": "干父之蛊；承以德也。"},
    {"hexagram_id": 18, "position": 6, "name": "上九", "text": "不事王侯，高尚其事。", "xiang_zhuan": "不事王侯，志可则也。"},
    
    {"hexagram_id": 19, "position": 1, "name": "初九", "text": "咸临，贞吉。", "xiang_zhuan": "咸临贞吉，志行正也。"},
    {"hexagram_id": 19, "position": 2, "name": "九二", "text": "咸临，吉无不利。", "xiang_zhuan": "咸临，吉无不利；未顺命也。"},
    {"hexagram_id": 19, "position": 3, "name": "六三", "text": "甘临，无攸利。既忧之，无咎。", "xiang_zhuan": "甘临，位不当也。既忧之，咎不长也。"},
    {"hexagram_id": 19, "position": 4, "name": "六四", "text": "至临，无咎。", "xiang_zhuan": "至临无咎，位当也。"},
    {"hexagram_id": 19, "position": 5, "name": "六五", "text": "知临，大君之宜，吉。", "xiang_zhuan": "大君之宜，行中之谓也。"},
    {"hexagram_id": 19, "position": 6, "name": "上六", "text": "敦临，吉无咎。", "xiang_zhuan": "敦临之吉，志在内也。"},
    
    {"hexagram_id": 20, "position": 1, "name": "初六", "text": "童观，小人无咎，君子吝。", "xiang_zhuan": "初六童观，小人道也。"},
    {"hexagram_id": 20, "position": 2, "name": "六二", "text": "窥观，利女贞。", "xiang_zhuan": "窥观女贞，亦可丑也。"},
    {"hexagram_id": 20, "position": 3, "name": "六三", "text": "观我生，进退。", "xiang_zhuan": "观我生，进退；未失道也。"},
    {"hexagram_id": 20, "position": 4, "name": "六四", "text": "观国之光，利用宾于王。", "xiang_zhuan": "观国之光，尚宾也。"},
    {"hexagram_id": 20, "position": 5, "name": "九五", "text": "观我生，君子无咎。", "xiang_zhuan": "观我生，观民也。"},
    {"hexagram_id": 20, "position": 6, "name": "上九", "text": "观其生，君子无咎。", "xiang_zhuan": "观其生，志未平也。"},
    
    {"hexagram_id": 21, "position": 1, "name": "初九", "text": "屦校灭趾，无咎。", "xiang_zhuan": "屦校灭趾，不行也。"},
    {"hexagram_id": 21, "position": 2, "name": "六二", "text": "噬肤灭鼻，无咎。", "xiang_zhuan": "噬肤灭鼻，乘刚也。"},
    {"hexagram_id": 21, "position": 3, "name": "六三", "text": "噬腊肉，遇毒；小吝，无咎。", "xiang_zhuan": "遇毒，位不当也。"},
    {"hexagram_id": 21, "position": 4, "name": "九四", "text": "噬乾胏，得金矢，利艰贞，吉。", "xiang_zhuan": "利艰贞，吉；未光也。"},
    {"hexagram_id": 21, "position": 5, "name": "六五", "text": "噬乾肉，得黄金，贞厉，无咎。", "xiang_zhuan": "贞厉，无咎；得当也。"},
    {"hexagram_id": 21, "position": 6, "name": "上九", "text": "何校灭耳，凶。", "xiang_zhuan": "何校灭耳，聪不明也。"},
    
    {"hexagram_id": 22, "position": 1, "name": "初九", "text": "贲其趾，舍车而徒。", "xiang_zhuan": "舍车而徒，义弗乘也。"},
    {"hexagram_id": 22, "position": 2, "name": "六二", "text": "贲其须。", "xiang_zhuan": "贲其须，与上兴也。"},
    {"hexagram_id": 22, "position": 3, "name": "九三", "text": "贲如濡如，永贞吉。", "xiang_zhuan": "永贞之吉，终莫之陵也。"},
    {"hexagram_id": 22, "position": 4, "name": "六四", "text": "贲如皤如，白马翰如，匪寇婚媾。", "xiang_zhuan": "六四，当位疑也。匪寇婚媾，终无尤也。"},
    {"hexagram_id": 22, "position": 5, "name": "六五", "text": "贲于丘园，束帛戋戋，吝，终吉。", "xiang_zhuan": "六五之吉，有喜也。"},
    {"hexagram_id": 22, "position": 6, "name": "上九", "text": "白贲，无咎。", "xiang_zhuan": "白贲无咎，上得志也。"},
    
    {"hexagram_id": 23, "position": 1, "name": "初六", "text": "剥床以足，蔑贞凶。", "xiang_zhuan": "剥床以足，以灭下也。"},
    {"hexagram_id": 23, "position": 2, "name": "六二", "text": "剥床以辨，蔑贞凶。", "xiang_zhuan": "剥床以辨，未有与也。"},
    {"hexagram_id": 23, "position": 3, "name": "六三", "text": "剥之，无咎。", "xiang_zhuan": "剥之无咎，失上下也。"},
    {"hexagram_id": 23, "position": 4, "name": "六四", "text": "剥床以肤，凶。", "xiang_zhuan": "剥床以肤，切近灾也。"},
    {"hexagram_id": 23, "position": 5, "name": "六五", "text": "贯鱼，以宫人宠，无不利。", "xiang_zhuan": "以宫人宠，终无尤也。"},
    {"hexagram_id": 23, "position": 6, "name": "上九", "text": "硕果不食，君子得舆，小人剥庐。", "xiang_zhuan": "君子得舆，民所载也。小人剥庐，终不可用也。"},
    
    {"hexagram_id": 24, "position": 1, "name": "初九", "text": "不远复，无祗悔，元吉。", "xiang_zhuan": "不远之复，以修身也。"},
    {"hexagram_id": 24, "position": 2, "name": "六二", "text": "休复，吉。", "xiang_zhuan": "休复之吉，以下仁也。"},
    {"hexagram_id": 24, "position": 3, "name": "六三", "text": "频复，厉无咎。", "xiang_zhuan": "频复之厉，义无咎也。"},
    {"hexagram_id": 24, "position": 4, "name": "六四", "text": "中行独复。", "xiang_zhuan": "中行独复，以从道也。"},
    {"hexagram_id": 24, "position": 5, "name": "六五", "text": "敦复，无悔。", "xiang_zhuan": "敦复无悔，中以自考也。"},
    {"hexagram_id": 24, "position": 6, "name": "上六", "text": "迷复，凶，有灾眚。用行师，终有大败，以其国君，凶；至于十年，不克征。", "xiang_zhuan": "迷复之凶，反君道也。"},
    
    {"hexagram_id": 25, "position": 1, "name": "初九", "text": "无妄，往吉。", "xiang_zhuan": "无妄之往，得志也。"},
    {"hexagram_id": 25, "position": 2, "name": "六二", "text": "不耕获，不菑畲，则利有攸往。", "xiang_zhuan": "不耕获，未富也。"},
    {"hexagram_id": 25, "position": 3, "name": "六三", "text": "无妄之灾，或系之牛，行人之得，邑人之灾。", "xiang_zhuan": "行人得牛，邑人灾也。"},
    {"hexagram_id": 25, "position": 4, "name": "九四", "text": "可贞，无咎。", "xiang_zhuan": "可贞无咎，固有之也。"},
    {"hexagram_id": 25, "position": 5, "name": "九五", "text": "无妄之疾，勿药有喜。", "xiang_zhuan": "无妄之药，不可试也。"},
    {"hexagram_id": 25, "position": 6, "name": "上九", "text": "无妄，行有眚，无攸利。", "xiang_zhuan": "无妄之行，穷之灾也。"},
    
    {"hexagram_id": 26, "position": 1, "name": "初九", "text": "有厉利已。", "xiang_zhuan": "有厉利已，不犯灾也。"},
    {"hexagram_id": 26, "position": 2, "name": "九二", "text": "舆说辐。", "xiang_zhuan": "舆说辐，中无尤也。"},
    {"hexagram_id": 26, "position": 3, "name": "九三", "text": "良马逐，利艰贞。曰闲舆卫，利有攸往。", "xiang_zhuan": "利有攸往，上合志也。"},
    {"hexagram_id": 26, "position": 4, "name": "六四", "text": "童牛之牿，元吉。", "xiang_zhuan": "六四元吉，有喜也。"},
    {"hexagram_id": 26, "position": 5, "name": "六五", "text": "豮豕之牙，吉。", "xiang_zhuan": "六五之吉，有庆也。"},
    {"hexagram_id": 26, "position": 6, "name": "上九", "text": "何天之衢，亨。", "xiang_zhuan": "何天之衢，道大行也。"},
    
    {"hexagram_id": 27, "position": 1, "name": "初九", "text": "舍尔灵龟，观我朵颐，凶。", "xiang_zhuan": "观我朵颐，亦不足贵也。"},
    {"hexagram_id": 27, "position": 2, "name": "六二", "text": "颠颐，拂经，于丘颐，征凶。", "xiang_zhuan": "六二征凶，行失类也。"},
    {"hexagram_id": 27, "position": 3, "name": "六三", "text": "拂颐，贞凶，十年勿用，无攸利。", "xiang_zhuan": "十年勿用，道大悖也。"},
    {"hexagram_id": 27, "position": 4, "name": "六四", "text": "颠颐，吉；虎视眈眈，其欲逐逐，无咎。", "xiang_zhuan": "颠颐之吉，上施光也。"},
    {"hexagram_id": 27, "position": 5, "name": "六五", "text": "拂经，居贞吉，不可涉大川。", "xiang_zhuan": "居贞之吉，顺以从上也。"},
    {"hexagram_id": 27, "position": 6, "name": "上九", "text": "由颐，厉吉，利涉大川。", "xiang_zhuan": "由颐厉吉，大有庆也。"},
    
    {"hexagram_id": 28, "position": 1, "name": "初六", "text": "藉用白茅，无咎。", "xiang_zhuan": "藉用白茅，柔在下也。"},
    {"hexagram_id": 28, "position": 2, "name": "九二", "text": "枯杨生稊，老夫得其女妻，无不利。", "xiang_zhuan": "老夫女妻，过以相与也。"},
    {"hexagram_id": 28, "position": 3, "name": "九三", "text": "栋桡，凶。", "xiang_zhuan": "栋桡之凶，不可以有辅也。"},
    {"hexagram_id": 28, "position": 4, "name": "九四", "text": "栋隆，吉；有它吝。", "xiang_zhuan": "栋隆之吉，不桡乎下也。"},
    {"hexagram_id": 28, "position": 5, "name": "九五", "text": "枯杨生华，老妇得其士夫，无咎无誉。", "xiang_zhuan": "枯杨生华，何可久也。老妇士夫，亦可丑也。"},
    {"hexagram_id": 28, "position": 6, "name": "上六", "text": "过涉灭顶，凶，无咎。", "xiang_zhuan": "过涉之凶，不可咎也。"},
    
    {"hexagram_id": 29, "position": 1, "name": "初六", "text": "习坎，入于坎窞，凶。", "xiang_zhuan": "习坎入坎，失道凶也。"},
    {"hexagram_id": 29, "position": 2, "name": "九二", "text": "坎有险，求小得。", "xiang_zhuan": "求小得，未出中也。"},
    {"hexagram_id": 29, "position": 3, "name": "六三", "text": "来之坎坎，险且枕，入于坎窞，勿用。", "xiang_zhuan": "来之坎坎，终无功也。"},
    {"hexagram_id": 29, "position": 4, "name": "六四", "text": "樽酒簋贰，用缶，纳约自牖，终无咎。", "xiang_zhuan": "樽酒簋贰，刚柔际也。"},
    {"hexagram_id": 29, "position": 5, "name": "九五", "text": "坎不盈，祗既平，无咎。", "xiang_zhuan": "坎不盈，中未大也。"},
    {"hexagram_id": 29, "position": 6, "name": "上六", "text": "系用徽纆，窴于丛棘，三岁不得，凶。", "xiang_zhuan": "上六失道，凶三岁也。"},
    
    {"hexagram_id": 30, "position": 1, "name": "初九", "text": "履错然，敬之无咎。", "xiang_zhuan": "履错之敬，以辟咎也。"},
    {"hexagram_id": 30, "position": 2, "name": "六二", "text": "黄离，元吉。", "xiang_zhuan": "黄离元吉，得中道也。"},
    {"hexagram_id": 30, "position": 3, "name": "九三", "text": "日昃之离，不鼓缶而歌，则大耋之嗟，凶。", "xiang_zhuan": "日昃之离，何可久也。"},
    {"hexagram_id": 30, "position": 4, "name": "九四", "text": "突如其来如，焚如，死如，弃如。", "xiang_zhuan": "突如其来如，无所容也。"},
    {"hexagram_id": 30, "position": 5, "name": "六五", "text": "出涕沱若，戚嗟若，吉。", "xiang_zhuan": "六五之吉，离王公也。"},
    {"hexagram_id": 30, "position": 6, "name": "上九", "text": "王用出征，有嘉折首，获匪其丑，无咎。", "xiang_zhuan": "王用出征，以正邦也。"},
    
    {"hexagram_id": 31, "position": 1, "name": "初六", "text": "咸其拇。", "xiang_zhuan": "咸其拇，志在外也。"},
    {"hexagram_id": 31, "position": 2, "name": "六二", "text": "咸其腓，凶，居吉。", "xiang_zhuan": "虽凶，居吉，顺不害也。"},
    {"hexagram_id": 31, "position": 3, "name": "九三", "text": "咸其股，执其随，往吝。", "xiang_zhuan": "咸其股，亦不处也。志在随人，所执下也。"},
    {"hexagram_id": 31, "position": 4, "name": "九四", "text": "贞吉悔亡，憧憧往来，朋从尔思。", "xiang_zhuan": "贞吉悔亡，未感害也。憧憧往来，未光大也。"},
    {"hexagram_id": 31, "position": 5, "name": "九五", "text": "咸其脢，无悔。", "xiang_zhuan": "咸其脢，志末也。"},
    {"hexagram_id": 31, "position": 6, "name": "上六", "text": "咸其辅，颊，舌。", "xiang_zhuan": "咸其辅，颊，舌，滕口说也。"},
    
    {"hexagram_id": 32, "position": 1, "name": "初六", "text": "浚恒，贞凶，无攸利。", "xiang_zhuan": "浚恒之凶，始求深也。"},
    {"hexagram_id": 32, "position": 2, "name": "九二", "text": "悔亡。", "xiang_zhuan": "九二悔亡，能久中也。"},
    {"hexagram_id": 32, "position": 3, "name": "九三", "text": "不恒其德，或承之羞，贞吝。", "xiang_zhuan": "不恒其德，无所容也。"},
    {"hexagram_id": 32, "position": 4, "name": "九四", "text": "田无禽。", "xiang_zhuan": "久非其位，安得禽也。"},
    {"hexagram_id": 32, "position": 5, "name": "六五", "text": "恒其德，贞，妇人吉，夫子凶。", "xiang_zhuan": "妇人贞吉，从一而终也。夫子制义，从妇凶也。"},
    {"hexagram_id": 32, "position": 6, "name": "上六", "text": "振恒，凶。", "xiang_zhuan": "振恒在上，大无功也。"},
    
    {"hexagram_id": 33, "position": 1, "name": "初六", "text": "遯尾，厉，勿用有攸往。", "xiang_zhuan": "遯尾之厉，不往何灾也。"},
    {"hexagram_id": 33, "position": 2, "name": "六二", "text": "执之用黄牛之革，莫之胜说。", "xiang_zhuan": "执用黄牛，固志也。"},
    {"hexagram_id": 33, "position": 3, "name": "九三", "text": "系遯，有疾厉，畜臣妾吉。", "xiang_zhuan": "系遯之厉，有疾惫也。畜臣妾吉，不可大事也。"},
    {"hexagram_id": 33, "position": 4, "name": "九四", "text": "好遯君子吉，小人否。", "xiang_zhuan": "君子好遯，小人否也。"},
    {"hexagram_id": 33, "position": 5, "name": "九五", "text": "嘉遯，贞吉。", "xiang_zhuan": "嘉遯贞吉，以正志也。"},
    {"hexagram_id": 33, "position": 6, "name": "上九", "text": "肥遯，无不利。", "xiang_zhuan": "肥遯，无不利；无所疑也。"},
    
    {"hexagram_id": 34, "position": 1, "name": "初九", "text": "壮于趾，征凶，有孚。", "xiang_zhuan": "壮于趾，其孚穷也。"},
    {"hexagram_id": 34, "position": 2, "name": "九二", "text": "贞吉。", "xiang_zhuan": "九二贞吉，以中也。"},
    {"hexagram_id": 34, "position": 3, "name": "九三", "text": "小人用壮，君子用罔，贞厉。羝羊触藩，羸其角。", "xiang_zhuan": "小人用壮，君子罔也。"},
    {"hexagram_id": 34, "position": 4, "name": "九四", "text": "贞吉悔亡，藩决不羸，壮于大舆之輹。", "xiang_zhuan": "藩决不羸，尚往也。"},
    {"hexagram_id": 34, "position": 5, "name": "六五", "text": "丧羊于易，无悔。", "xiang_zhuan": "丧羊于易，位不当也。"},
    {"hexagram_id": 34, "position": 6, "name": "上六", "text": "羝羊触藩，不能退，不能遂，无攸利，艰则吉。", "xiang_zhuan": "不能退，不能遂，不详也。艰则吉，咎不长也。"},
    
    {"hexagram_id": 35, "position": 1, "name": "初六", "text": "晋如，摧如，贞吉。罔孚，裕无咎。", "xiang_zhuan": "晋如，摧如；独行正也。裕无咎；未受命也。"},
    {"hexagram_id": 35, "position": 2, "name": "六二", "text": "晋如，愁如，贞吉。受兹介福，于其王母。", "xiang_zhuan": "受兹介福，以中正也。"},
    {"hexagram_id": 35, "position": 3, "name": "六三", "text": "众允，悔亡。", "xiang_zhuan": "众允之，志上行也。"},
    {"hexagram_id": 35, "position": 4, "name": "九四", "text": "晋如鼫鼠，贞厉。", "xiang_zhuan": "鼫鼠贞厉，位不当也。"},
    {"hexagram_id": 35, "position": 5, "name": "六五", "text": "悔亡，失得勿恤，往吉无不利。", "xiang_zhuan": "失得勿恤，往有庆也。"},
    {"hexagram_id": 35, "position": 6, "name": "上九", "text": "晋其角，维用伐邑，厉吉无咎，贞吝。", "xiang_zhuan": "维用伐邑，道未光也。"},
    
    {"hexagram_id": 36, "position": 1, "name": "初九", "text": "明夷于飞，垂其翼。君子于行，三日不食，有攸往，主人有言。", "xiang_zhuan": "君子于行，义不食也。"},
    {"hexagram_id": 36, "position": 2, "name": "六二", "text": "明夷，夷于左股，用拯马壮，吉。", "xiang_zhuan": "六二之吉，顺以则也。"},
    {"hexagram_id": 36, "position": 3, "name": "九三", "text": "明夷于南狩，得其大首，不可疾贞。", "xiang_zhuan": "南狩之志，乃大得也。"},
    {"hexagram_id": 36, "position": 4, "name": "六四", "text": "入于左腹，获明夷之心，出于门庭。", "xiang_zhuan": "入于左腹，获心意也。"},
    {"hexagram_id": 36, "position": 5, "name": "六五", "text": "箕子之明夷，利贞。", "xiang_zhuan": "箕子之贞，明不可息也。"},
    {"hexagram_id": 36, "position": 6, "name": "上六", "text": "不明晦，初登于天，后入于地。", "xiang_zhuan": "初登于天，照四国也。后入于地，失则也。"},
    
    {"hexagram_id": 37, "position": 1, "name": "初九", "text": "闲有家，悔亡。", "xiang_zhuan": "闲有家，志未变也。"},
    {"hexagram_id": 37, "position": 2, "name": "六二", "text": "无攸遂，在中馈，贞吉。", "xiang_zhuan": "六二之吉，顺以巽也。"},
    {"hexagram_id": 37, "position": 3, "name": "九三", "text": "家人嗃嗃，悔厉吉；妇子嘻嘻，终吝。", "xiang_zhuan": "家人嗃嗃，未失也；妇子嘻嘻，失家节也。"},
    {"hexagram_id": 37, "position": 4, "name": "六四", "text": "富家，大吉。", "xiang_zhuan": "富家大吉，顺在位也。"},
    {"hexagram_id": 37, "position": 5, "name": "九五", "text": "王假有家，勿恤吉。", "xiang_zhuan": "王假有家，交相爱也。"},
    {"hexagram_id": 37, "position": 6, "name": "上九", "text": "有孚威如，终吉。", "xiang_zhuan": "威如之吉，反身之谓也。"},
    
    {"hexagram_id": 38, "position": 1, "name": "初九", "text": "悔亡，丧马勿逐，自复；见恶人无咎。", "xiang_zhuan": "见恶人，以辟咎也。"},
    {"hexagram_id": 38, "position": 2, "name": "九二", "text": "遇主于巷，无咎。", "xiang_zhuan": "遇主于巷，未失道也。"},
    {"hexagram_id": 38, "position": 3, "name": "六三", "text": "见舆曳，其牛掣，其人天且劓，无初有终。", "xiang_zhuan": "见舆曳，位不当也。无初有终，遇刚也。"},
    {"hexagram_id": 38, "position": 4, "name": "九四", "text": "睽孤，遇元夫，交孚，厉无咎。", "xiang_zhuan": "交孚无咎，志行也。"},
    {"hexagram_id": 38, "position": 5, "name": "六五", "text": "悔亡，厥宗噬肤，往何咎。", "xiang_zhuan": "厥宗噬肤，往有庆也。"},
    {"hexagram_id": 38, "position": 6, "name": "上九", "text": "睽孤，见豕负涂，载鬼一车，先张之弧，后说之弧，匪寇婚媾，往遇雨则吉。", "xiang_zhuan": "遇雨之吉，群疑亡也。"},
    
    {"hexagram_id": 39, "position": 1, "name": "初六", "text": "往蹇，来誉。", "xiang_zhuan": "往蹇来誉，宜待也。"},
    {"hexagram_id": 39, "position": 2, "name": "六二", "text": "王臣蹇蹇，匪躬之故。", "xiang_zhuan": "王臣蹇蹇，终无尤也。"},
    {"hexagram_id": 39, "position": 3, "name": "九三", "text": "往蹇来反。", "xiang_zhuan": "往蹇来反，内喜之也。"},
    {"hexagram_id": 39, "position": 4, "name": "六四", "text": "往蹇来连。", "xiang_zhuan": "往蹇来连，当位实也。"},
    {"hexagram_id": 39, "position": 5, "name": "九五", "text": "大蹇朋来。", "xiang_zhuan": "大蹇朋来，以中节也。"},
    {"hexagram_id": 39, "position": 6, "name": "上六", "text": "往蹇来硕，吉；利见大人。", "xiang_zhuan": "往蹇来硕，志在内也。利见大人，以从贵也。"},
    
    {"hexagram_id": 40, "position": 1, "name": "初六", "text": "无咎。", "xiang_zhuan": "刚柔之际，义无咎也。"},
    {"hexagram_id": 40, "position": 2, "name": "九二", "text": "田获三狐，得黄矢，贞吉。", "xiang_zhuan": "九二贞吉，得中道也。"},
    {"hexagram_id": 40, "position": 3, "name": "六三", "text": "负且乘，致寇至，贞吝。", "xiang_zhuan": "负且乘，亦可丑也，自我致戎，又谁咎也。"},
    {"hexagram_id": 40, "position": 4, "name": "九四", "text": "解而拇，朋至斯孚。", "xiang_zhuan": "解而拇，未当位也。"},
    {"hexagram_id": 40, "position": 5, "name": "六五", "text": "君子维有解，吉；有孚于小人。", "xiang_zhuan": "君子有解，小人退也。"},
    {"hexagram_id": 40, "position": 6, "name": "上六", "text": "公用射隼，于高墉之上，获之，无不利。", "xiang_zhuan": "公用射隼，以解悖也。"},
    
    {"hexagram_id": 41, "position": 1, "name": "初九", "text": "已事遄往，无咎，酌损之。", "xiang_zhuan": "已事遄往，尚合志也。"},
    {"hexagram_id": 41, "position": 2, "name": "九二", "text": "利贞，征凶，弗损益之。", "xiang_zhuan": "九二利贞，中以为志也。"},
    {"hexagram_id": 41, "position": 3, "name": "六三", "text": "三人行，则损一人；一人行，则得其友。", "xiang_zhuan": "一人行，三则疑也。"},
    {"hexagram_id": 41, "position": 4, "name": "六四", "text": "损其疾，使遄有喜，无咎。", "xiang_zhuan": "损其疾，亦可喜也。"},
    {"hexagram_id": 41, "position": 5, "name": "六五", "text": "或益之，十朋之龟弗克违，元吉。", "xiang_zhuan": "六五元吉，自上祐也。"},
    {"hexagram_id": 41, "position": 6, "name": "上九", "text": "弗损益之，无咎，贞吉，利有攸往，得臣无家。", "xiang_zhuan": "弗损益之，大得志也。"},
    
    {"hexagram_id": 42, "position": 1, "name": "初九", "text": "利用为大作，元吉，无咎。", "xiang_zhuan": "元吉无咎，下不厚事也。"},
    {"hexagram_id": 42, "position": 2, "name": "六二", "text": "或益之，十朋之龟弗克违，永贞吉。王用享于帝，吉。", "xiang_zhuan": "或益之，自外来也。"},
    {"hexagram_id": 42, "position": 3, "name": "六三", "text": "益之用凶事，无咎。有孚中行，告公用圭。", "xiang_zhuan": "益用凶事，固有之也。"},
    {"hexagram_id": 42, "position": 4, "name": "六四", "text": "中行，告公从。利用为依迁国。", "xiang_zhuan": "告公从，以益志也。"},
    {"hexagram_id": 42, "position": 5, "name": "九五", "text": "有孚惠心，勿问元吉。有孚惠我德。", "xiang_zhuan": "有孚惠心，勿问之矣。惠我德，大得志也。"},
    {"hexagram_id": 42, "position": 6, "name": "上九", "text": "莫益之，或击之，立心勿恒，凶。", "xiang_zhuan": "莫益之，偏辞也。或击之，自外来也。"},
    
    {"hexagram_id": 43, "position": 1, "name": "初九", "text": "壮于前趾，往不胜为吝。", "xiang_zhuan": "不胜而往，咎也。"},
    {"hexagram_id": 43, "position": 2, "name": "九二", "text": "惕号，莫夜有戎，勿恤。", "xiang_zhuan": "莫夜有戎，得中道也。"},
    {"hexagram_id": 43, "position": 3, "name": "九三", "text": "壮于頄，有凶。君子夬夬，独行遇雨，若濡有愠，无咎。", "xiang_zhuan": "君子夬夬，终无咎也。"},
    {"hexagram_id": 43, "position": 4, "name": "九四", "text": "臀无肤，其行次且。牵羊悔亡，闻言不信。", "xiang_zhuan": "其行次且，位不当也。闻言不信，聪不明也。"},
    {"hexagram_id": 43, "position": 5, "name": "九五", "text": "苋陆夬夬，中行无咎。", "xiang_zhuan": "中行无咎，中未光也。"},
    {"hexagram_id": 43, "position": 6, "name": "上六", "text": "无号，终有凶。", "xiang_zhuan": "无号之凶，终不可长也。"},
    
    {"hexagram_id": 44, "position": 1, "name": "初六", "text": "系于金柅，贞吉，有攸往，见凶，羸豕孚蹢躅。", "xiang_zhuan": "系于金柅，柔道牵也。"},
    {"hexagram_id": 44, "position": 2, "name": "九二", "text": "包有鱼，无咎，不利宾。", "xiang_zhuan": "包有鱼，义不及宾也。"},
    {"hexagram_id": 44, "position": 3, "name": "九三", "text": "臀无肤，其行次且，厉，无大咎。", "xiang_zhuan": "其行次且，行未牵也。"},
    {"hexagram_id": 44, "position": 4, "name": "九四", "text": "包无鱼，起凶。", "xiang_zhuan": "无鱼之凶，远民也。"},
    {"hexagram_id": 44, "position": 5, "name": "九五", "text": "以杞包瓜，含章，有陨自天。", "xiang_zhuan": "九五含章，中正也。有陨自天，志不舍命也。"},
    {"hexagram_id": 44, "position": 6, "name": "上九", "text": "姤其角，吝，无咎。", "xiang_zhuan": "姤其角，上穷吝也。"},
    
    {"hexagram_id": 45, "position": 1, "name": "初六", "text": "有孚不终，乃乱乃萃，若号一握为笑，勿恤，往无咎。", "xiang_zhuan": "乃乱乃萃，其志乱也。"},
    {"hexagram_id": 45, "position": 2, "name": "六二", "text": "引吉，无咎，孚乃利用禴。", "xiang_zhuan": "引吉无咎，中未变也。"},
    {"hexagram_id": 45, "position": 3, "name": "六三", "text": "萃如，嗟如，无攸利，往无咎，小吝。", "xiang_zhuan": "往无咎，上巽也。"},
    {"hexagram_id": 45, "position": 4, "name": "九四", "text": "大吉，无咎。", "xiang_zhuan": "大吉无咎，位不当也。"},
    {"hexagram_id": 45, "position": 5, "name": "九五", "text": "萃有位，无咎。匪孚，元永贞，悔亡。", "xiang_zhuan": "萃有位，志未光也。"},
    {"hexagram_id": 45, "position": 6, "name": "上六", "text": "赍咨涕洟，无咎。", "xiang_zhuan": "赍咨涕洟，未安上也。"},
    
    {"hexagram_id": 46, "position": 1, "name": "初六", "text": "允升，大吉。", "xiang_zhuan": "允升大吉，上合志也。"},
    {"hexagram_id": 46, "position": 2, "name": "九二", "text": "孚乃利用禴，无咎。", "xiang_zhuan": "九二之孚，有喜也。"},
    {"hexagram_id": 46, "position": 3, "name": "九三", "text": "升虚邑。", "xiang_zhuan": "升虚邑，无所疑也。"},
    {"hexagram_id": 46, "position": 4, "name": "六四", "text": "王用亨于岐山，吉无咎。", "xiang_zhuan": "王用亨于岐山，顺事也。"},
    {"hexagram_id": 46, "position": 5, "name": "六五", "text": "贞吉，升阶。", "xiang_zhuan": "贞吉升阶，大得志也。"},
    {"hexagram_id": 46, "position": 6, "name": "上六", "text": "冥升，利于不息之贞。", "xiang_zhuan": "冥升在上，消不富也。"},
    
    {"hexagram_id": 47, "position": 1, "name": "初六", "text": "臀困于株木，入于幽谷，三岁不见。", "xiang_zhuan": "入于幽谷，幽不明也。"},
    {"hexagram_id": 47, "position": 2, "name": "九二", "text": "困于酒食，朱绂方来，利用亨祀，征凶，无咎。", "xiang_zhuan": "困于酒食，中有庆也。"},
    {"hexagram_id": 47, "position": 3, "name": "六三", "text": "困于石，据于蒺藜，入于其宫，不见其妻，凶。", "xiang_zhuan": "据于蒺藜，乘刚也。入于其宫，不见其妻，不祥也。"},
    {"hexagram_id": 47, "position": 4, "name": "九四", "text": "来徐徐，困于金车，吝，有终。", "xiang_zhuan": "来徐徐，志在下也。虽不当位，有与也。"},
    {"hexagram_id": 47, "position": 5, "name": "九五", "text": "劓刖，困于赤绂，乃徐有说，利用祭祀。", "xiang_zhuan": "劓刖，志未得也。乃徐有说，以中直也。利用祭祀，受福也。"},
    {"hexagram_id": 47, "position": 6, "name": "上六", "text": "困于葛藟，于臲卼，曰动悔。有悔，征吉。", "xiang_zhuan": "困于葛藟，未当也。动悔，有悔吉，行也。"},
    
    {"hexagram_id": 48, "position": 1, "name": "初六", "text": "井泥不食，旧井无禽。", "xiang_zhuan": "井泥不食，下也。旧井无禽，时舍也。"},
    {"hexagram_id": 48, "position": 2, "name": "九二", "text": "井谷射鲋，瓮敝漏。", "xiang_zhuan": "井谷射鲋，无与也。"},
    {"hexagram_id": 48, "position": 3, "name": "九三", "text": "井渫不食，为我心恻，可用汲，王明，并受其福。", "xiang_zhuan": "井渫不食，行恻也。求王明，受福也。"},
    {"hexagram_id": 48, "position": 4, "name": "六四", "text": "井甃，无咎。", "xiang_zhuan": "井甃无咎，修井也。"},
    {"hexagram_id": 48, "position": 5, "name": "九五", "text": "井冽，寒泉食。", "xiang_zhuan": "寒泉之食，中正也。"},
    {"hexagram_id": 48, "position": 6, "name": "上六", "text": "井收勿幕，有孚元吉。", "xiang_zhuan": "元吉在上，大成也。"},
    
    {"hexagram_id": 49, "position": 1, "name": "初九", "text": "巩用黄牛之革。", "xiang_zhuan": "巩用黄牛，不可以有为也。"},
    {"hexagram_id": 49, "position": 2, "name": "六二", "text": "已日乃革之，征吉，无咎。", "xiang_zhuan": "已日革之，行有嘉也。"},
    {"hexagram_id": 49, "position": 3, "name": "九三", "text": "征凶，贞厉，革言三就，有孚。", "xiang_zhuan": "革言三就，又何之矣。"},
    {"hexagram_id": 49, "position": 4, "name": "九四", "text": "悔亡，有孚改命，吉。", "xiang_zhuan": "改命之吉，信志也。"},
    {"hexagram_id": 49, "position": 5, "name": "九五", "text": "大人虎变，未占有孚。", "xiang_zhuan": "大人虎变，其文炳也。"},
    {"hexagram_id": 49, "position": 6, "name": "上六", "text": "君子豹变，小人革面，征凶，居贞吉。", "xiang_zhuan": "君子豹变，其文蔚也。小人革面，顺以从君也。"},
    
    {"hexagram_id": 50, "position": 1, "name": "初六", "text": "鼎颠趾，利出否，得妾以其子，无咎。", "xiang_zhuan": "鼎颠趾，未悖也。利出否，以从贵也。"},
    {"hexagram_id": 50, "position": 2, "name": "九二", "text": "鼎有实，我仇有疾，不我能即，吉。", "xiang_zhuan": "鼎有实，慎所之也。我仇有疾，终无尤也。"},
    {"hexagram_id": 50, "position": 3, "name": "九三", "text": "鼎耳革，其行塞，雉膏不食，方雨亏悔，终吉。", "xiang_zhuan": "鼎耳革，失其义也。"},
    {"hexagram_id": 50, "position": 4, "name": "九四", "text": "鼎折足，覆公餗，其形渥，凶。", "xiang_zhuan": "覆公餗，信如何也。"},
    {"hexagram_id": 50, "position": 5, "name": "六五", "text": "鼎黄耳金铉，利贞。", "xiang_zhuan": "鼎黄耳，中以为实也。"},
    {"hexagram_id": 50, "position": 6, "name": "上九", "text": "鼎玉铉，大吉，无不利。", "xiang_zhuan": "玉铉在上，刚柔节也。"},
    
    {"hexagram_id": 51, "position": 1, "name": "初九", "text": "震来虩虩，后笑言哑哑，吉。", "xiang_zhuan": "震来虩虩，恐致福也。笑言哑哑，后有则也。"},
    {"hexagram_id": 51, "position": 2, "name": "六二", "text": "震来厉，亿丧贝，跻于九陵，勿逐，七日得。", "xiang_zhuan": "震来厉，乘刚也。"},
    {"hexagram_id": 51, "position": 3, "name": "六三", "text": "震苏苏，震行无眚。", "xiang_zhuan": "震苏苏，位不当也。"},
    {"hexagram_id": 51, "position": 4, "name": "九四", "text": "震遂泥。", "xiang_zhuan": "震遂泥，未光也。"},
    {"hexagram_id": 51, "position": 5, "name": "六五", "text": "震往来厉，亿无丧，有事。", "xiang_zhuan": "震往来厉，危行也。其事在中，大无丧也。"},
    {"hexagram_id": 51, "position": 6, "name": "上六", "text": "震索索，视矍矍，征凶。震不于其躬，于其邻，无咎。婚媾有言。", "xiang_zhuan": "震索索，未得中也。虽凶无咎，畏邻戒也。"},
    
    {"hexagram_id": 52, "position": 1, "name": "初六", "text": "艮其趾，无咎，利永贞。", "xiang_zhuan": "艮其趾，未失正也。"},
    {"hexagram_id": 52, "position": 2, "name": "六二", "text": "艮其腓，不拯其随，其心不快。", "xiang_zhuan": "不拯其随，未退听也。"},
    {"hexagram_id": 52, "position": 3, "name": "九三", "text": "艮其限，列其夤，厉薰心。", "xiang_zhuan": "艮其限，危薰心也。"},
    {"hexagram_id": 52, "position": 4, "name": "六四", "text": "艮其身，无咎。", "xiang_zhuan": "艮其身，止诸躬也。"},
    {"hexagram_id": 52, "position": 5, "name": "六五", "text": "艮其辅，言有序，悔亡。", "xiang_zhuan": "艮其辅，以中正也。"},
    {"hexagram_id": 52, "position": 6, "name": "上九", "text": "敦艮，吉。", "xiang_zhuan": "敦艮之吉，以厚终也。"},
    
    {"hexagram_id": 53, "position": 1, "name": "初六", "text": "鸿渐于干，小子厉，有言，无咎。", "xiang_zhuan": "小子之厉，义无咎也。"},
    {"hexagram_id": 53, "position": 2, "name": "六二", "text": "鸿渐于磐，饮食衎衎，吉。", "xiang_zhuan": "饮食衎衎，不素饱也。"},
    {"hexagram_id": 53, "position": 3, "name": "九三", "text": "鸿渐于陆，夫征不复，妇孕不育，凶；利御寇。", "xiang_zhuan": "夫征不复，离群丑也。妇孕不育，失其道也。利用御寇，顺相保也。"},
    {"hexagram_id": 53, "position": 4, "name": "六四", "text": "鸿渐于木，或得其桷，无咎。", "xiang_zhuan": "或得其桷，顺以巽也。"},
    {"hexagram_id": 53, "position": 5, "name": "九五", "text": "鸿渐于陵，妇三岁不孕，终莫之胜，吉。", "xiang_zhuan": "终莫之胜，吉；得所愿也。"},
    {"hexagram_id": 53, "position": 6, "name": "上九", "text": "鸿渐于陆，其羽可用为仪，吉。", "xiang_zhuan": "其羽可用为仪，吉；不可乱也。"},
    
    {"hexagram_id": 54, "position": 1, "name": "初九", "text": "归妹以娣，跛能履，征吉。", "xiang_zhuan": "归妹以娣，以恒也。跛能履吉，相承也。"},
    {"hexagram_id": 54, "position": 2, "name": "九二", "text": "眇能视，利幽人之贞。", "xiang_zhuan": "利幽人之贞，未变常也。"},
    {"hexagram_id": 54, "position": 3, "name": "六三", "text": "归妹以须，反归以娣。", "xiang_zhuan": "归妹以须，未当也。"},
    {"hexagram_id": 54, "position": 4, "name": "九四", "text": "归妹愆期，迟归有时。", "xiang_zhuan": "愆期之志，有待而行也。"},
    {"hexagram_id": 54, "position": 5, "name": "六五", "text": "帝乙归妹，其君之袂，不如其娣之袂良，月几望，吉。", "xiang_zhuan": "帝乙归妹，不如其娣之袂良也。其位在中，以贵行也。"},
    {"hexagram_id": 54, "position": 6, "name": "上六", "text": "女承筐无实，士刲羊无血，无攸利。", "xiang_zhuan": "上六无实，承虚筐也。"},
    
    {"hexagram_id": 55, "position": 1, "name": "初九", "text": "遇其配主，虽旬无咎，往有尚。", "xiang_zhuan": "虽旬无咎，过旬灾也。"},
    {"hexagram_id": 55, "position": 2, "name": "六二", "text": "丰其蔀，日中见斗，往得疑疾，有孚发若，吉。", "xiang_zhuan": "有孚发若，信以发志也。"},
    {"hexagram_id": 55, "position": 3, "name": "九三", "text": "丰其沛，日中见沬，折其右肱，无咎。", "xiang_zhuan": "丰其沛，不可大事也。折其右肱，终不可用也。"},
    {"hexagram_id": 55, "position": 4, "name": "九四", "text": "丰其蔀，日中见斗，遇其夷主，吉。", "xiang_zhuan": "丰其蔀，位不当也。日中见斗，幽不明也。遇其夷主，吉；行也。"},
    {"hexagram_id": 55, "position": 5, "name": "六五", "text": "来章，有庆誉，吉。", "xiang_zhuan": "六五之吉，有庆也。"},
    {"hexagram_id": 55, "position": 6, "name": "上六", "text": "丰其屋，蔀其家，窥其户，阒其无人，三岁不见，凶。", "xiang_zhuan": "丰其屋，天际翔也。窥其户，阒其无人，自藏也。"},
    
    {"hexagram_id": 56, "position": 1, "name": "初六", "text": "旅琐琐，斯其所取灾。", "xiang_zhuan": "旅琐琐，志穷灾也。"},
    {"hexagram_id": 56, "position": 2, "name": "六二", "text": "旅即次，怀其资，得童仆贞。", "xiang_zhuan": "得童仆贞，终无尤也。"},
    {"hexagram_id": 56, "position": 3, "name": "九三", "text": "旅焚其次，丧其童仆，贞厉。", "xiang_zhuan": "旅焚其次，亦以伤矣。以旅与下，其义丧也。"},
    {"hexagram_id": 56, "position": 4, "name": "九四", "text": "旅于处，得其资斧，我心不快。", "xiang_zhuan": "旅于处，未得位也。得其资斧，心未快也。"},
    {"hexagram_id": 56, "position": 5, "name": "六五", "text": "射雉一矢亡，终以誉命。", "xiang_zhuan": "终以誉命，上逮也。"},
    {"hexagram_id": 56, "position": 6, "name": "上九", "text": "鸟焚其巢，旅人先笑后号啕。丧牛于易，凶。", "xiang_zhuan": "以旅在上，其义焚也。丧牛于易，终莫之闻也。"},
    
    {"hexagram_id": 57, "position": 1, "name": "初六", "text": "进退，利武人之贞。", "xiang_zhuan": "进退，志疑也。利武人之贞，志治也。"},
    {"hexagram_id": 57, "position": 2, "name": "九二", "text": "巽在床下，用史巫纷若，吉无咎。", "xiang_zhuan": "纷若之吉，得中也。"},
    {"hexagram_id": 57, "position": 3, "name": "九三", "text": "频巽，吝。", "xiang_zhuan": "频巽之吝，志穷也。"},
    {"hexagram_id": 57, "position": 4, "name": "六四", "text": "悔亡，田获三品。", "xiang_zhuan": "田获三品，有功也。"},
    {"hexagram_id": 57, "position": 5, "name": "九五", "text": "贞吉悔亡，无不利。无初有终，先庚三日，后庚三日，吉。", "xiang_zhuan": "九五之吉，位正中也。"},
    {"hexagram_id": 57, "position": 6, "name": "上九", "text": "巽在床下，丧其资斧，贞凶。", "xiang_zhuan": "巽在床下，上穷也。丧其资斧，正乎凶也。"},
    
    {"hexagram_id": 58, "position": 1, "name": "初九", "text": "和兑，吉。", "xiang_zhuan": "和兑之吉，行未疑也。"},
    {"hexagram_id": 58, "position": 2, "name": "九二", "text": "孚兑，吉，悔亡。", "xiang_zhuan": "孚兑之吉，信志也。"},
    {"hexagram_id": 58, "position": 3, "name": "六三", "text": "来兑，凶。", "xiang_zhuan": "来兑之凶，位不当也。"},
    {"hexagram_id": 58, "position": 4, "name": "九四", "text": "商兑未宁，介疾有喜。", "xiang_zhuan": "九四之喜，有庆也。"},
    {"hexagram_id": 58, "position": 5, "name": "九五", "text": "孚于剥，有厉。", "xiang_zhuan": "孚于剥，位正当也。"},
    {"hexagram_id": 58, "position": 6, "name": "上六", "text": "引兑。", "xiang_zhuan": "上六引兑，未光也。"},
    
    {"hexagram_id": 59, "position": 1, "name": "初六", "text": "用拯马壮，吉。", "xiang_zhuan": "初六之吉，顺也。"},
    {"hexagram_id": 59, "position": 2, "name": "九二", "text": "涣奔其机，悔亡。", "xiang_zhuan": "涣奔其机，得愿也。"},
    {"hexagram_id": 59, "position": 3, "name": "六三", "text": "涣其躬，无悔。", "xiang_zhuan": "涣其躬，志在外也。"},
    {"hexagram_id": 59, "position": 4, "name": "六四", "text": "涣其群，元吉。涣有丘，匪夷所思。", "xiang_zhuan": "涣其群，元吉；光大也。"},
    {"hexagram_id": 59, "position": 5, "name": "九五", "text": "涣汗其大号，涣王居，无咎。", "xiang_zhuan": "王居无咎，正位也。"},
    {"hexagram_id": 59, "position": 6, "name": "上九", "text": "涣其血，去逖出，无咎。", "xiang_zhuan": "涣其血，远害也。"},
    
    {"hexagram_id": 60, "position": 1, "name": "初九", "text": "不出户庭，无咎。", "xiang_zhuan": "不出户庭，知通塞也。"},
    {"hexagram_id": 60, "position": 2, "name": "九二", "text": "不出门庭，凶。", "xiang_zhuan": "不出门庭，失时极也。"},
    {"hexagram_id": 60, "position": 3, "name": "六三", "text": "不节若，则嗟若，无咎。", "xiang_zhuan": "不节之嗟，又谁咎也。"},
    {"hexagram_id": 60, "position": 4, "name": "六四", "text": "安节，亨。", "xiang_zhuan": "安节之亨，承上道也。"},
    {"hexagram_id": 60, "position": 5, "name": "九五", "text": "甘节，吉；往有尚。", "xiang_zhuan": "甘节之吉，居位中也。"},
    {"hexagram_id": 60, "position": 6, "name": "上六", "text": "苦节，贞凶，悔亡。", "xiang_zhuan": "苦节贞凶，其道穷也。"},
    
    {"hexagram_id": 61, "position": 1, "name": "初九", "text": "虞吉，有它不燕。", "xiang_zhuan": "初九虞吉，志未变也。"},
    {"hexagram_id": 61, "position": 2, "name": "九二", "text": "鸣鹤在阴，其子和之，我有好爵，吾与尔靡之。", "xiang_zhuan": "其子和之，中心愿也。"},
    {"hexagram_id": 61, "position": 3, "name": "六三", "text": "得敌，或鼓或罢，或泣或歌。", "xiang_zhuan": "或鼓或罢，位不当也。"},
    {"hexagram_id": 61, "position": 4, "name": "六四", "text": "月几望，马匹亡，无咎。", "xiang_zhuan": "马匹亡，绝类上也。"},
    {"hexagram_id": 61, "position": 5, "name": "九五", "text": "有孚挛如，无咎。", "xiang_zhuan": "有孚挛如，位正当也。"},
    {"hexagram_id": 61, "position": 6, "name": "上九", "text": "翰音登于天，贞凶。", "xiang_zhuan": "翰音登于天，何可长也。"},
    
    {"hexagram_id": 62, "position": 1, "name": "初六", "text": "飞鸟以凶。", "xiang_zhuan": "飞鸟以凶，不可如何也。"},
    {"hexagram_id": 62, "position": 2, "name": "六二", "text": "过其祖，遇其妣；不及其君，遇其臣；无咎。", "xiang_zhuan": "不及其君，臣不可过也。"},
    {"hexagram_id": 62, "position": 3, "name": "九三", "text": "弗过防之，从或戕之，凶。", "xiang_zhuan": "从或戕之，凶如何也。"},
    {"hexagram_id": 62, "position": 4, "name": "九四", "text": "无咎，弗过遇之。往厉必戒，勿用永贞。", "xiang_zhuan": "弗过遇之，位不当也。往厉必戒，终不可长也。"},
    {"hexagram_id": 62, "position": 5, "name": "六五", "text": "密云不雨，自我西郊，公弋取彼在穴。", "xiang_zhuan": "密云不雨，已上也。"},
    {"hexagram_id": 62, "position": 6, "name": "上六", "text": "弗遇过之，飞鸟离之，凶，是谓灾眚。", "xiang_zhuan": "弗遇过之，已亢也。"},
    
    {"hexagram_id": 63, "position": 1, "name": "初九", "text": "曳其轮，濡其尾，无咎。", "xiang_zhuan": "曳其轮，义无咎也。"},
    {"hexagram_id": 63, "position": 2, "name": "六二", "text": "妇丧其茀，勿逐，七日得。", "xiang_zhuan": "七日得，以中道也。"},
    {"hexagram_id": 63, "position": 3, "name": "九三", "text": "高宗伐鬼方，三年克之，小人勿用。", "xiang_zhuan": "三年克之，惫也。"},
    {"hexagram_id": 63, "position": 4, "name": "六四", "text": "繻有衣袽，终日戒。", "xiang_zhuan": "终日戒，有所疑也。"},
    {"hexagram_id": 63, "position": 5, "name": "九五", "text": "东邻杀牛，不如西邻之禴祭，实受其福。", "xiang_zhuan": "东邻杀牛，不如西邻之时也；实受其福，吉大来也。"},
    {"hexagram_id": 63, "position": 6, "name": "上六", "text": "濡其首，厉。", "xiang_zhuan": "濡其首厉，何可久也。"},
    
    {"hexagram_id": 64, "position": 1, "name": "初六", "text": "濡其尾，吝。", "xiang_zhuan": "濡其尾，亦不知极也。"},
    {"hexagram_id": 64, "position": 2, "name": "九二", "text": "曳其轮，贞吉。", "xiang_zhuan": "九二贞吉，中以行正也。"},
    {"hexagram_id": 64, "position": 3, "name": "六三", "text": "未济，征凶，利涉大川。", "xiang_zhuan": "未济征凶，位不当也。"},
    {"hexagram_id": 64, "position": 4, "name": "九四", "text": "贞吉，悔亡，震用伐鬼方，三年有赏于大国。", "xiang_zhuan": "贞吉悔亡，志行也。"},
    {"hexagram_id": 64, "position": 5, "name": "六五", "text": "贞吉，无悔，君子之光，有孚，吉。", "xiang_zhuan": "君子之光，其晖吉也。"},
    {"hexagram_id": 64, "position": 6, "name": "上九", "text": "有孚于饮酒，无咎，濡其首，有孚失是。", "xiang_zhuan": "饮酒濡首，亦不知节也。"}
  ]
}
